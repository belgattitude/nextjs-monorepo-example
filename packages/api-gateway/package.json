  "devDependencies": {
    "@types/node": "20.8.7",
    "@your-org/eslint-config-bases": "workspace:^",
    "cross-env": "7.0.3",
    "eslint": "8.52.0",
    "graphql": "16.8.1",
    "npm-run-all": "4.1.5",
