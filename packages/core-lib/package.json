    "react-dom": "18.3.1",
    "rimraf": "6.0.1",
    "tsup": "8.2.4",
    "typescript": "5.5.4",
    "vite": "5.4.1",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "2.0.5"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
