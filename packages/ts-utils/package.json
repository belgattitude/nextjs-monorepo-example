{
  "name": "@your-org/ts-utils",
  "version": "1.1.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "license": "MIT",
  "homepage": "https://github.com/belgattitude/nextjs-monorepo-example",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/nextjs-monorepo-example",
    "directory": "packages/ts-utils"
  },
  "scripts": {
    "build": "tsup",
    "check-dist": "es-check -v",
    "check-size": "size-limit",
    "clean": "rimraf ./dist ./coverage ./tsconfig.tsbuildinfo",
    "dev": "tsup --watch",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/ts-utils.eslintcache",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js",
    "test-unit-watch": "vitest watch --ui",
    "typecheck": "tsc --project ./tsconfig.json --noEmit"
  },
  "devDependencies": {
    "@size-limit/file": "8.2.4",
    "@types/jest": "29.5.2",
    "@types/node": "20.2.3",
    "@vitest/coverage-istanbul": "0.31.4",
    "@vitest/ui": "0.31.4",
    "@your-org/eslint-config-bases": "workspace:^",
    "cross-env": "7.0.3",
    "es-check": "7.1.1",
    "eslint": "8.41.0",
    "get-tsconfig": "4.6.0",
    "jest": "29.5.0",
    "rimraf": "5.0.1",
    "size-limit": "8.2.4",
    "ts-jest": "29.1.0",
    "tslib": "2.5.2",
    "tsup": "6.7.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.4"
  },
  "engines": {
    "node": ">=14.16"
  }
}
