{
  "name": "@your-org/ts-utils",
  "version": "1.1.1",
  "license": "MIT",
  "private": true,
  "homepage": "https://github.com/belgattitude/nextjs-monorepo-example",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/nextjs-monorepo-example",
    "directory": "packages/ts-utils"
  },
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf --no-glob ./dist ./coverage ./tsconfig.tsbuildinfo",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --cache --cache-location ../../.cache/eslint/ts-utils.eslintcache",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "check-dist": "es-check -v",
    "check-size": "size-limit",
    "test": "vitest run",
    "test-unit": "vitest run",
    "test-unit-jest": "jest --config ./jest.config.js",
    "test-unit-watch": "vitest watch --ui",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.mjs,.cjs,.mts,.cts --fix"
  },
  "devDependencies": {
    "@size-limit/file": "8.1.0",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@vitest/coverage-istanbul": "0.25.0",
    "@vitest/ui": "0.25.0",
    "@your-org/eslint-config-bases": "workspace:^",
    "cross-env": "7.0.3",
    "es-check": "7.0.1",
    "eslint": "8.27.0",
    "get-tsconfig": "4.2.0",
    "jest": "29.3.1",
    "rimraf": "3.0.2",
    "size-limit": "8.1.0",
    "ts-jest": "29.0.3",
    "tslib": "2.4.1",
    "tsup": "6.4.0",
    "typescript": "4.8.4",
    "vite": "3.2.3",
    "vite-tsconfig-paths": "3.5.2",
    "vitest": "0.25.0"
  },
  "engines": {
    "node": ">=14.16"
  }
}
