{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "build": {
      // "env": ["NEXT_PUBLIC_STRAPI_API_URL"],
      "outputs": [".next/**", "!.next/cache/**"],
      "dependsOn": ["@your-org/api-gateway#build"]
    },
    "typecheck": {
      "dependsOn": ["@your-org/api-gateway#build"]
    },
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["@your-org/api-gateway#dev"]
    }
  }
}
