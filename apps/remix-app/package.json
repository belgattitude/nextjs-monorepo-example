{
  "name": "remix-app",
  "version": "1.10.11",
  "private": true,
  "sideEffects": false,
  "author": {
    "name": "Vanvelthem SÃ©bastien",
    "url": "https://github.com/belgattitude"
  },
  "license": "MIT",
  "homepage": "https://github.com/belgattitude/nextjs-monorepo-example",
  "repository": {
    "type": "git",
    "url": "https://github.com/belgattitude/nextjs-monorepo-example",
    "directory": "apps/remix-app"
  },
  "scripts": {
    "build": "npm run build-css && remix build",
    "build-css": "tailwindcss -o ./src/tailwind.css",
    "clean": "rimraf ./.cache ./coverage ./src/tailwind.css ./api/index.js ./api/index.js.map ./public/build ./tsconfig.tsbuildinfo",
    "dev": "npm-run-all --parallel --print-label watch-tailwind remix-dev",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --fix",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs,.mdx,.graphql --cache --cache-location ../../.cache/eslint/remix-app.eslintcache",
    "lint-styles": "stylelint 'src/**/*.css'",
    "remix-dev": "remix dev",
    "test": "yarn test-unit",
    "test-unit": "jest --config jest.config.js",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "watch-tailwind": "tailwindcss -o ./src/tailwind.css --watch"
  },
  "dependencies": {
    "@emotion/cache": "11.11.0",
    "@emotion/react": "11.11.1",
    "@emotion/server": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@remix-run/node": "1.19.1",
    "@remix-run/react": "1.19.1",
    "@remix-run/serve": "1.19.1",
    "@vercel/remix": "1.19.1",
    "@your-org/common-i18n": "workspace:^",
    "@your-org/core-lib": "workspace:^",
    "@your-org/ui-lib": "workspace:^",
    "isbot": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "remix": "1.19.1"
  },
  "devDependencies": {
    "@babel/core": "7.22.9",
    "@emotion/babel-plugin": "11.11.0",
    "@remix-run/dev": "1.19.1",
    "@remix-run/eslint-config": "1.19.1",
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.4",
    "@testing-library/dom": "9.3.1",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/jest": "29.5.3",
    "@types/node": "20.4.5",
    "@types/react": "18.2.17",
    "@types/react-dom": "18.2.7",
    "@types/testing-library__jest-dom": "5.14.9",
    "@your-org/eslint-config-bases": "workspace:^",
    "autoprefixer": "10.4.14",
    "eslint": "8.45.0",
    "eslint-plugin-tailwindcss": "3.13.0",
    "get-tsconfig": "4.6.2",
    "jest": "29.6.2",
    "jest-css-modules-transform": "4.4.2",
    "jest-environment-jsdom": "29.6.2",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.27",
    "prettier": "3.0.0",
    "stylelint": "15.10.2",
    "stylelint-config-standard": "34.0.0",
    "tailwindcss": "3.3.3",
    "ts-jest": "29.1.1",
    "typescript": "5.1.6"
  }
