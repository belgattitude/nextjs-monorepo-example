    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "27.4.1",
    "@types/node": "17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "autoprefixer": "10.4.2",
    "cross-env": "7.0.3",
    "es-check": "6.2.1",
    "eslint": "8.10.0",
