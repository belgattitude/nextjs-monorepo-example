{
  "name": "vite-app",
  "version": "0.2.1",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "clean": "rimraf --no-glob ./dist ./tsconfig.tsbuildinfo ./node_modules/.vite",
    "serve": "vite preview",
    "test": "run-s 'test:*'",
    "test:unit": "echo 'No test present yet'",
    "ci-test:unit": "echo 'No test present yet'",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "fix:staged-files": "lint-staged --allow-empty",
    "fix:all-files": "eslint . --ext .ts,.tsx,.js,.jsx --fix"
  },
  "dependencies": {
    "@emotion/react": "11.7.1",
    "@emotion/styled": "11.6.0",
    "@your-org/core-lib": "workspace:*",
    "@your-org/ui-lib": "workspace:*",
    "react": "18.0.0-rc.0-next-fe905f152-20220107",
    "react-dom": "18.0.0-rc.0-next-fe905f152-20220107",
    "wouter": "2.8.0-alpha.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.7",
    "@emotion/babel-plugin": "11.7.2",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@vitejs/plugin-react": "1.1.4",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "25.3.4",
    "eslint-plugin-jest-formatting": "3.1.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-regexp": "1.5.1",
    "eslint-plugin-sonarjs": "0.11.0",
    "eslint-plugin-testing-library": "5.0.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "typescript": "4.5.4",
    "vite": "2.7.10",
    "vite-tsconfig-paths": "3.3.17"
  }
}
