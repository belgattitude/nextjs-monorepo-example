{
  "name": "vite-app",
  "version": "0.6.11",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "clean": "rimraf --no-glob ./dist ./tsconfig.tsbuildinfo ./node_modules/.vite",
    "serve": "vite preview",
    "test": "yarn test-unit",
    "test-unit": "echo 'No test present yet'",
    "typecheck": "tsc --project ./tsconfig.json --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --cache --cache-location ../../.cache/eslint/vite-app.eslintcache",
    "fix-all-files": "eslint . --ext .ts,.tsx,.js,.jsx,.cjs,.mjs --fix"
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@your-org/common-i18n": "workspace:^",
    "@your-org/core-lib": "workspace:^",
    "@your-org/ui-lib": "workspace:^",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.20.5",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.9",
    "@vitejs/plugin-react": "3.0.0",
    "@vitest/coverage-istanbul": "0.26.2",
    "@vitest/ui": "0.26.2",
    "@your-org/eslint-config-bases": "workspace:^",
    "eslint": "8.30.0",
    "happy-dom": "8.1.1",
    "prettier": "2.8.1",
    "rimraf": "3.0.2",
    "typescript": "4.9.4",
    "vite": "4.0.3",
    "vite-plugin-svgr": "2.4.0",
    "vite-tsconfig-paths": "4.0.3",
    "vitest": "0.26.2"
  }
}
